#include<iostream>
using namespace std;
int main()
{
  int n,min,max,i;
  cout<<"enter the size of array : \n";
  cin>>n;
  int arr[n];
  cout<<"enter elements in array : \n";
  for(i=0;i<n;i++)
  {
    cin>>arr[i];
  }
  min=max=arr[0];
  for(i=0;i<n;i++)
  {
    if(arr[i]>max)
    {
      max=arr[i];
    }
    if(arr[i]<min)
    {
      min=arr[i];
    }
  }
  cout<<"max element = "<<max<<endl;
  cout<<"min element = "<<min<<endl;
  return 0;
}
