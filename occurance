#include<iostream>
using namespace std;
int main()
{
  int n,arr[10],target,counter=0,i;
  cout<<"enter the size of array : \n";
  cin>>n;
  cout<<"enter the elements in array : \n";
  for(i=0;i<n;i++)
  {
    cin>>arr[i];
  }
  cout<<"enter the target value : \n";
  cin>>target;
  for(i=0;i<n;i++)
  {
    if(arr[i]==target)
    {
      counter++;
    }
  }
  cout<<"number "<<target<<"occurs "<<counter<<" times in the array \n";
  return 0;
}
